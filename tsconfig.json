{
    "compilerOptions": {
        "target": "ESNext",
        "module": "CommonJS",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "baseUrl": "./",
        "types": ["cypress"],

        "noUnusedLocals": true,
        "noUnusedParameters": true,
        
        // Added for ts-unused-exports
        "declaration": true,
        "declarationMap": true,
        "emitDeclarationOnly": true,
        "outDir": "dist-types"
    },
    "include": ["cypress/**/*.ts"]
  }
  